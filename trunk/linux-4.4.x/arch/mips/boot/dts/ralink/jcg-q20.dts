/dts-v1/;

#include "mt7621.dtsi"
#include <dt-bindings/input/input.h>
#include "mt7621-raeth-gmac2.dtsi"

/ {
	compatible = "mediatek,mt7621-soc";
	model = "JCG Q20";

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	leds {
		compatible = "gpio-leds";

		sys_red {
			label = "power_2";
			gpios = <&gpio0 14 GPIO_ACTIVE_LOW>;
		};

		sys_blue {
			label = "power";
			gpios = <&gpio0 15 GPIO_ACTIVE_LOW>;
		};
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio0 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		wps {
			label = "wps";
			gpios = <&gpio0 16 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};
};

&bch {
	status = "okay";
};

&nand {
	status = "okay";

	flash@0 {
		reg = <0>;
		nand-ecc-mode = "hw";
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "Bootloader";
				reg = <0x0 0x80000>; /* 512k */
				read-only;
			};

			partition@80000 {
				label = "Config";
				reg = <0x80000 0x80000>; /* 512k */
			};

			partition@100000 {
				label = "Factory";
				reg = <0x100000 0x80000>; /* 512k */
			};

			partition@180000 {
				label = "firmware";
				reg = <0x180000 0x1800000>;	/* 24M */
			};

			partition@1980000 {
				label = "Storage";
				reg = <0x1980000 0x400000>; /* 4096K */
			};

			partition@1d80000 {
				label = "RWFS";
				reg = <0x1d80000 0x6200000>;
			};

			partition@all {
				label = "ALL";
				reg = <0x0 0x7f80000>;
			};
		};
	};
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			mtk,group = "i2c", "uart2", "uart3", "rgmii2";
			mtk,function = "gpio";
		};
	};
};

&pcie {
	status = "okay";
};
